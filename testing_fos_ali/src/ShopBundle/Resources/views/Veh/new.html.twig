{% extends 'admin.html.twig' %}


{% block strong %}Véhicule {%  endblock %}

    {% block h2 %} Information{% endblock %}

    {% block path %} {{ path('veh_index') }} {% endblock %}

{% block body %}




    <div class="row clearfix">
                <div class="col-lg-12 col-md-12 col-sm-12">
                    <div class="card">
                        <div class="block-header">
                        </div>
                        <div class="body">

                            {{ form_start(form,{'attr': {'class': 'form-horizontal', 'novalidate':'novalidate'}}) }}


                            <div class="row clearfix">
                                <div class="col-lg-2 col-md-2 col-sm-4 form-control-label">
                                    <label for="email_address_2">Véhicule Image</label>
                                </div>
                                <div class="col-lg-10 col-md-10 col-sm-8">

                                        {{ form_widget(form.ad,{'attr': {'class': 'form-control'}}) }}

                                </div>
                            </div>
                            <div class="row clearfix">
                                <div class="col-lg-2 col-md-2 col-sm-4 form-control-label">
                                    <label for="email_address_2">Date Début Contrat</label>
                                </div>
                                <div class="col-lg-10 col-md-10 col-sm-8">

                                        {{ form_widget(form.debutcontrat,{'attr': {'class': 'form-control','placeholder':'Ex: 2016-01-30'}}) }}


                                </div>

                            </div>
                            <div class="row clearfix">
                                <div class="col-lg-2 col-md-2 col-sm-4 form-control-label">
                                    <label for="email_address_2">Date Fin Contrat</label>
                                </div>

                                <div class="col-lg-10 col-md-10 col-sm-8">

                                        {{ form_widget(form.fincontrat,{'attr': {'class': 'form-control','placeholder':'Ex: 2016-01-30'}}) }}

                                </div>
                            </div>
                            <div class="row clearfix">
                                <div class="col-lg-2 col-md-2 col-sm-4 form-control-label">
                                    <label for="password_2">Série</label>
                                </div>
                                <div class="col-lg-10 col-md-10 col-sm-8">

                                        {{ form_widget(form.serie,{'attr': {'class': 'form-control'}}) }}

                                </div>
                            </div>
                            <div class="row clearfix">
                                <div class="col-lg-2 col-md-2 col-sm-4 form-control-label">
                                    <label for="password_2">Societé Parent</label>
                                </div>
                                <div class="col-lg-10 col-md-10 col-sm-8">

                                        {{ form_widget(form.ids,{'attr': {'class': 'form-control'}}) }}

                                </div>
                            </div>
                            <div class="row clearfix">

                                <div class="col-sm-8 offset-sm-2">
                                    <input type="submit" class="btn btn-raised btn-primary btn-round waves-effect" value="Ajouter">
                                </div>
                            </div>
                            {{ form_end(form) }}

                        </div>
                    </div>
                </div>
            </div>

{% endblock %}
